class Solution {
    public List<Integer> luckyNumbers(int[][] matrix) {
        ArrayList<Integer> LuckyNumbers = new ArrayList<>();

        int n = matrix.length;
        for(int i = 0; i < n; i++){
            int min = matrix[i][0]; 
            int cols = 0;
            for(int j = 0; j < matrix[0].length; j++){
                if(min > matrix[i][j]){
                    min = matrix[i][j]; 
                    cols = j;
                }
            }
            boolean max = true;

            for(int k = 0; k < n; k++){
                if(matrix[k][cols] > min){
                    max = false;
                    break;
                }
            }
            if(max){
                LuckyNumbers.add(min);
            }
        }
        return LuckyNumbers;
    }
}
